  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/vertexai": "^1.8.0",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.2.0"
  },
  "private": true
}